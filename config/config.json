{
    "requestURI": "/boat",
    "urls": "import:./urls",

    "btMerchantId": "xzy2672yq6ct227c",
    "btPublicKey": "42hvjkd7gt3gk6q9",
    "btPrivateKey": "812157ed805e2b9360dcb568a80d36ee",


    
    "express": {
        "view cache": false,
        "view engine": "js",
        "views": "path:./.build/templates",
        "mountpath": "/boat"
    },
    

    
    "view engines": {
        "js": {
            "module": "engine-munger",
            "renderer": {
                "method": "js",
                "arguments": [
                    { "cache": true },
                    {
                        "views": "config:express.views",
                        "view engine": "config:express.view engine",
                        "specialization": "config:specialization",
                        "i18n": "config:i18n"
                    }
                ]
            }
        }
    },

    "i18n": {
        "contentPath": "path:./locales",
        "fallback": "en-US"
    },
    

    "specialization": {
    },

    "middleware": {

        "expressView": {
            "priority": 100,
            "enabled": true,
            "module": {
                "name": "engine-munger/lib/expressView"
            }
        },

        "populateCommonData": {
            "priority": 101,
            "enabled": true,
            "module": {
                "name": "../../../controllers/middleware/populate-common-data"
            }
        },

        "static": {
            "module": {
                "arguments": [ "path:./.build" ]
            }
        },

        "router": {
            "module": {
                "arguments": [{ "directory": "path:./routes" }]
            }
        }

    }
}
